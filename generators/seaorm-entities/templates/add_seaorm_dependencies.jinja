{# add dependencies in Cargo.toml #}
message: "Add seaorm dependencies in Cargo.toml."
injections:
- into: {{ values.outputFolder }}/Cargo.toml
  after: \[dependencies\]
  content: |
    sea-orm = { version = "1.1.4", features = ["sqlx-postgres","runtime-tokio-rustls","macros","with-uuid","with-chrono"] }
    chrono = { version = "0.4", features = ["serde"] }
    uuid = { version = "1.13", features = ["v4", "serde"] }
---
